<h1>Edit your succulent here.</h1>
<p><%= link_to 'Back to your Succulents', succulents_path %></p>

<%= form_for @succulent do |f| %>
  <%= f.label :name %> <br />
  <%= f.text_field :name %> <br />

  <%= f.label :description %> <br />
  <%= f.text_area :description %><br />

  <%= f.submit 'Save' %>
<% end %>
